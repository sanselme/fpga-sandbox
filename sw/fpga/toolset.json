{
  "schemaVersion": "1.0",
  "swiftCompiler": {
    "extraCLIOptions": [
      "-enable-experimental-feature", "Embedded",
      "-enable-experimental-feature", "Extern",
      "-Xclang-linker", "-nostdlib",
      "-Xfrontend", "-function-sections",
      "-Xfrontend", "-mergeable-symbols",
      "-Xfrontend", "-enable-single-module-llvm-emission",
      "-assert-config", "Debug"
    ]
  },
  "cCompiler": {
    "extraCLIOptions": [
      "-nostdlib",
      "-ffunction-sections",
      "-Wall"
    ]
  },
  "linker": {
    "extraCLIOptions": [
      "-v",
      "-t",
      "-lc",
      "-lm"
    ]
  }
}
